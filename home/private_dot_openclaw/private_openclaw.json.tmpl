{
  "env": {
    "shellEnv": {
      "enabled": true,
      "timeoutMs": 2000
    },
    "vars": {
      "GEMINI_API_KEY":       "{{ onepasswordRead "op://Private/Gemini API/credential" }}",
      "GITHUB_TOKEN":         "{{ onepasswordRead "op://Private/GitHub PAT (openclaw)/credential" }}",
      "GOG_KEYRING_PASSWORD": "{{ onepasswordRead "op://Private/GOG Keyring/password" }}",
      "OPENROUTER_API_KEY":   "{{ onepasswordRead "op://Private/OpenRouter API/credential" }}",
      "ELEVENLABS_API_KEY":   "{{ onepasswordRead "op://Private/ElevenLabs API/credential" }}"
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "anthropic/claude-sonnet-4-6",
        "fallbacks": ["openrouter/minimax/minimax-m2.5"]
      },
      "models": {
        "anthropic/claude-opus-4-5":          { "alias": "opus" },
        "openai-codex/gpt-5.2":               { "alias": "codex" },
        "openrouter/moonshotai/kimi-k2.5":    { "alias": "kimi" },
        "openrouter/minimax/minimax-m2.5":    { "alias": "minimax" },
        "anthropic/claude-sonnet-4-6":        { "alias": "sonnet" },
        "opencode/claude-opus-4-6":           { "alias": "oc-opus" }
      },
      "workspace": "/home/ubuntu/.openclaw/workspace",
      "userTimezone": "Australia/Sydney",
      "contextPruning": {
        "mode": "cache-ttl",
        "ttl": "30m"
      },
      "compaction": {
        "mode": "safeguard",
        "reserveTokensFloor": 24000,
        "memoryFlush": {
          "enabled": true,
          "softThresholdTokens": 10000,
          "prompt": "Write any lasting notes to memory/YYYY-MM-DD.md; reply with NO_REPLY if nothing to store.",
          "systemPrompt": "Session nearing compaction. Store durable memories now."
        }
      },
      "verboseDefault": "off",
      "typingIntervalSeconds": 4,
      "typingMode": "thinking",
      "heartbeat": {
        "every": "1h",
        "activeHours": { "start": "06:30", "end": "23:00" },
        "model": "anthropic/claude-sonnet-4-6",
        "includeReasoning": true,
        "target": "last"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8,
        "runTimeoutSeconds": 1200
      }
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "name": "Main",
        "workspace": "/home/ubuntu/.openclaw/workspace"
      }
    ]
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "botToken": "{{ onepasswordRead "op://Private/Sparky Telegram Bot/credential" }}",
      "dmPolicy": "pairing",
      "groupPolicy": "allowlist",
      "replyToMode": "all",
      "streaming": "partial",
      "reactionNotifications": "all",
      "linkPreview": true,
      "markdown": { "tables": "code" },
      "groups": {
        "-1003878938995": {
          "requireMention": false,
          "topics": {
            "31": {
              "requireMention": false,
              "systemPrompt": "You're working on essence.ai — the Next.js marketing site for Essence.ai.\n\nRepo: ~/src/essence.ai\nGitHub: https://github.com/danethomas/essence.ai\nLive: https://tryessence.ai\n\nStack: Next.js 14, Tailwind, Framer Motion\nDeploy: Cloudflare Pages (auto-deploys from main, preview on branches)\n\nFocus: Copy refinements, landing page animations, visual polish.\nAlways use feature branches. PR for review before merging to main."
            },
            "34": {
              "requireMention": false,
              "systemPrompt": "You're working on essence-app — the Rails monolith for Essence.ai.\n\nStack: Rails 8, Postgres + pgvector, Inertia.js + React + Vite, Hotwire, Solid Queue\nRepo: ~/src/essence-app\nGitHub: https://github.com/danethomas/essence-app\n\nArchitecture docs:\n- /home/ubuntu/.openclaw/workspace/projects/essence/architecture.md\n- /home/ubuntu/.openclaw/workspace/projects/essence/primitives.md\n\nFocus: Backend architecture, service objects, API design, database modeling.\nPrefer Codex for heavy building — set with /model codex."
            }
          }
        }
      }
    },
    "discord": {
      "enabled": true,
      "token": "{{ onepasswordRead "op://Private/Sparky Discord Bot/credential" }}",
      "groupPolicy": "allowlist",
      "streaming": "off",
      "guilds": {
        "1476100150886793277": {
          "requireMention": false,
          "users": ["197173392821059584"]
        }
      }
    }
  },
  "tools": {
    "web": {
      "search": {
        "apiKey": "{{ onepasswordRead "op://Private/Brave Search API/credential" }}"
      }
    },
    "media": {
      "audio": { "enabled": true }
    },
    "agentToAgent": {
      "enabled": true
    },
    "exec": {
      "security": "allowlist",
      "ask": "on-miss",
      "safeBins": [
        "awk", "bash", "bird", "bun", "cat", "chmod", "clawhub", "cp", "curl",
        "cut", "date", "echo", "find", "git", "gog", "grep", "head", "ln",
        "ls", "mkdir", "mv", "node", "npm", "npx", "openclaw", "printf",
        "python", "python3", "rm", "sed", "sh", "sort", "tail", "tee",
        "touch", "tr", "trash", "uniq", "wc", "wget", "xargs"
      ],
      "safeBinProfiles": {
        "awk": {}, "bash": {}, "bird": {}, "bun": {}, "cat": {}, "chmod": {},
        "clawhub": {}, "cp": {}, "curl": {}, "cut": {}, "date": {}, "echo": {},
        "find": {}, "git": {}, "gog": {}, "grep": {}, "head": {}, "ln": {},
        "ls": {}, "mkdir": {}, "mv": {}, "node": {}, "npm": {}, "npx": {},
        "openclaw": {}, "printf": {}, "python": {}, "python3": {}, "rm": {},
        "sed": {}, "sh": {}, "sort": {}, "tail": {}, "tee": {}, "touch": {},
        "tr": {}, "trash": {}, "uniq": {}, "wc": {}, "wget": {}, "xargs": {}
      }
    }
  }
}
